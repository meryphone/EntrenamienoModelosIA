# Función para calcular límites de winsorización
calcular_limites <- function(x, lower_quantile = 0.05, upper_quantile = 0.95) {
lower_limit <- quantile(x, lower_quantile, na.rm = TRUE)
upper_limit <- quantile(x, upper_quantile, na.rm = TRUE)
return(list(lower_limit = lower_limit, upper_limit = upper_limit))
}
# Aplicar winsorización
winsorizar_test <- function(x, limites) {
x[x < limites$lower_limit] <- limites$lower_limit
x[x > limites$upper_limit] <- limites$upper_limit
return(x)
}
